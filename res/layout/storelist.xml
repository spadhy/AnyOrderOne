<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/LinearLayout01" android:orientation="vertical" android:layout_width="wrap_content"
	android:layout_height="wrap_content">
	<TextView android:id="@+id/textView1"
		android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/select_rest"></TextView>
	<ListView android:id="@android:id/list" android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:layout_weight="1"
		android:drawSelectorOnTop="false" />
	<TextView android:id="@+id/empty" android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:text="" />
</LinearLayout>
